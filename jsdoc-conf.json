{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./js"],
        "includePattern": ".+\\.js|\\.vue?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "destination": "./docs",
        "template": "./node_modules/tui-jsdoc-template"
    }
}